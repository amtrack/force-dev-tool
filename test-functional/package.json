{
  "name": "test",
  "version": "1.0.0",
  "description": "functional tests with cucumber",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/cucumber-js feature/*.feature --tags 'not @skipped' --require step-definitions/changeSet.js --format node_modules/cucumber-pretty",
    "debug": "NODE_OPTIONS=--inspect-brk ./node_modules/.bin/cucumber-js feature/*.feature --tags 'not @skipped' --require step-definitions/changeSet.js --format node_modules/cucumber-pretty",
    "doing": "./node_modules/.bin/cucumber-js feature/*.feature --tags '@doing' --require step-definitions/changeSet.js --format node_modules/cucumber-pretty"
  },
  "license": "MIT",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "tmp": "^0.1.0"
  }
}
