{
  "name": "force-dev-tool",
  "description": "Command line tool supporting the Force.com development lifecycle",
  "version": "0.0.0-development",
  "author": "Matthias Rolke <mr.amtrack@gmail.com>",
  "bin": {
    "force-dev-tool": "bin/cli"
  },
  "dependencies": {
    "async": "^2.6.0",
    "chalk": "^2.3.0",
    "docopt": "^0.6.2",
    "event-stream": "3.3.4",
    "find-up": "^3.0.0",
    "fs-extra": "^6.0.1",
    "glob": "^7.0.6",
    "jsforce": "^1.7.0",
    "mississippi": "^3.0.0",
    "mkdirp": "^0.5.1",
    "multimatch": "^2.1.0",
    "parse-diff": "^0.4.2",
    "split2": "^3.0.0",
    "tmp": "0.0.33",
    "underscore": "^1.8.3",
    "vinyl": "^1.2.0",
    "vinyl-fs": "^2.4.3",
    "xml2js": "^0.4.19",
    "xmldoc": "^1.1.0",
    "yauzl": "^2.9.1",
    "yazl": "^2.4.3"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "glob-all": "^3.1.0",
    "istanbul": "^0.4.4",
    "js-beautify": "^1.7.5",
    "mocha": "^5.0.0",
    "sinon": "^6.0.1"
  },
  "keywords": [
    "cli",
    "deployment",
    "diff",
    "force.com",
    "forcedotcom",
    "metadata",
    "salesforce",
    "sfdc",
    "lifecycle"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/amtrack/force-dev-tool.git"
  },
  "scripts": {
    "istanbul": "istanbul test ./node_modules/.bin/_mocha -- test/ --recursive",
    "lint": "eslint --ignore-path .gitignore .",
    "mocha": "mocha test/ --recursive",
    "test": "npm run lint && npm run istanbul",
    "test:integration": "mocha test-integration --recursive",
    "fmt": "glob-all 'lib/**/*.{js,json}' 'test/**/*.{js,json}' 'test-integration/**/*.{js,json}' 'bin/cli' | xargs js-beautify -r"
  }
}
